<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ll_week"
    android:layout_width="match_parent"
    android:layout_height="@dimen/qb_px_30"
    android:background="#FFFFFF"
    android:minHeight="@dimen/qb_px_30"
    android:orientation="horizontal"
    android:paddingBottom="@dimen/qb_px_2"
    android:paddingTop="@dimen/qb_px_2"
    tools:ignore="HardcodedText">

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/sun"
        android:textColor="#FF333333"
        android:layout_marginLeft="@dimen/qb_px_5"
        android:textSize="@dimen/qb_px_12" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/mon"
        android:textColor="#FF333333"
        android:textSize="@dimen/qb_px_12"  />

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/tue"
        android:textColor="#FF333333"
        android:textSize="@dimen/qb_px_12"  />

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/wed"
        android:textColor="#FF333333"
        android:textSize="@dimen/qb_px_12"  />

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/thu"
        android:textColor="#FF333333"
        android:textSize="@dimen/qb_px_12"  />

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/fri"
        android:textColor="#FF333333"
        android:textSize="@dimen/qb_px_12"  />

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/sat"
        android:textColor="#FF333333"
        android:layout_marginRight="@dimen/qb_px_5"
        android:textSize="@dimen/qb_px_12"  />
</merge>